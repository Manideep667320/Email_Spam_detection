{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.9",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.py"
    }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9"
    },
    "use": "@vercel/static-build",
    "config": {
      "distDir": ".",
      "zeroConfig": true
    }
  },
  "buildCommand": "chmod +x ./build.sh && ./build.sh"
}